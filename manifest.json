{
  "name": "API 插件",
  "description": "API插件，用于发起http请求，生成code sample",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "notifications",
    "webRequest"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_icon": {
      "16": "/images/Love16.png",
      "32": "/images/Love32.png",
      "48": "/images/Love48.png",
      "128": "/images/Love128.png"
    }
  },
  "icons": {
    "16": "/images/Love16.png",
    "32": "/images/Love32.png",
    "48": "/images/Love48.png",
    "128": "/images/Love128.png"
  },
  "options_page": "src/index.html",
  "web_accessible_resources": [
    {
      "resources": [
        "*.html"
      ],
      "matches": [
        "https://*/*"
      ]
    }
  ]
}
